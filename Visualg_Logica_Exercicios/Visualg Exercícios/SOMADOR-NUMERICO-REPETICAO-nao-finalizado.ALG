Algoritmo "somador-numerico-repeticao"
//  
//  
// O programa irá ler 10 numeros e soma-los a cada repetição
//
//
Var

 cont:inteiro
 n:inteiro
 s:inteiro

Inicio

  cont <- 1
  s <- 0
  enquanto (cont <= 5) faca
  escreval (" Digite o numero que deseja somar")
  leia (n)
  s <- s + n
  cont <- (cont+1)
  fimenquanto
  escreval (s)

Fimalgoritmo