Algoritmo "par-impar"
//
//
// Escrever numeros impares entre 100 e 200
//

Var
   cont, imp:inteiro



Inicio


      para cont <- 100 ate 200 passo 1 faca
      imp <- (cont)

      se (imp % 2 = 1)entao
      escreval ("Número impar", imp)

      fimse
      

      
      fimpara

      
      






Fimalgoritmo