Algoritmo "EXERCICIO-9-NEW-TAB"
//
//
// Contar de 1 a 100
// E imprimir a cada multiplo de 10 a mensagem: multiplo de 10..
Var
   cont, n:inteiro


Inicio
   n <- 0
   cont <- 1
   repita

      escreval(cont)
      cont <- cont+1

   ate (cont>10)
   escreval ("multiplo de 10")
   repita
 escreval(cont)
      cont <- cont+1
   
   
   ate (cont >100)






Fimalgoritmo